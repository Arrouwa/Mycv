*{
	padding:0;
	margin:0;
}
.container{
	background:green;
	height:100vh;
	width:100%;
}
.logo{
	width:100%;
	height:4%;
	background:#f1f1f1;
	float:left;
}
.nav{
	width:100%;
	height:8%;
	background:#4C6793;
	float:left;
}
.nav ul{
	float:right;
	list-style:none;
}
.nav ul li{
	display:inline-block;
	padding:14px 0px;
}
.nav ul li a{
	color:white;
	text-decoration:none;
	margin-right:20px;
	text-transform:capitalize;
}
.nav ul li a:hover{
	text-decoration:underline;
}
.content{
	width:100%;
	height:83%;
	background:#f1f1f1;
	float:left;
}
.footer{
	width:100%;
	height:5%;
	background:#4C6793;
	float:left;
}